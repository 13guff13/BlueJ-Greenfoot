#!/bin/sh

BINDIR=`dirname $0`
. $BINDIR/config

JAVA_BIN=/usr/java
SUNTOOLS=$JAVA_BIN/lib/tools.jar
BLUEJ_HOME=$BINDIR/.. 
CLASSROOT=$BLUEJ_HOME/classes
JPDA=$BLUEJ_HOME/lib/jpda.jar
ANTLR=$BLUEJ_HOME/lib/antlr.jar

CLASSPATH=`makepath "$CLASSROOT;$JPDA;$ANTLR;$SUNTOOLS;$CLASSPATH"`
export CLASSPATH

LD_LIBRARY_PATH=$LD_LIBRARY_PATH:$BLUEJ_HOME/lib/sparc
export LD_LIBRARY_PATH

THREADS_FLAG=native; export THREADS_FLAG

java -noverify -Dbluej.home=`makepath $BLUEJ_HOME` bluej.Main $@
