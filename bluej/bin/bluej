#!/bin/sh

BINDIR=`dirname $0`
. $BINDIR/config

BLUEJ_HOME=$BINDIR/.. 
CLASSROOT=$BLUEJ_HOME/classes
LIB_DIR=$BLUEJ_HOME/lib
JBUG=$BLUEJ_HOME/lib/jbug.jar

XCLASSPATH=""
CLASSPATH=`makepath "$CLASSROOT;$JBUG;$LIB_DIR;$CLASSPATH"`
export CLASSPATH

java -noverify -Dbluej.home=`makepath $BLUEJ_HOME` bluej.Main $@
