#####################################################################
## System libraries which are added to BlueJ's class path
#####################################################################

bluej.systemlibrary1.description=A Sample Library for Windows
bluej.systemlibrary1.location=S\:\\Programming\\Java\\lib\\sample.jar

bluej.systemlibrary2.description=A Sample Library for UNIX
bluej.systemlibrary2.location=/usr/local/java/lib/sample.jar

#######################################################################
## Fonts. With Java, you don't have much choice of font faces, but you
## can set the size.
#######################################################################

# font size for most of the interface components
bluej.fontsize=12
#bluej.fontsize=10

# The editor font. This is the name of a font face with an optional "-bold"
# at the end. Most used are monospaced fonts, such as Courier. Good large
# fonts for screen projections are SansSerif-bold, 14pt, or Courier-bold, 18pt.
# Some examples:
#
# bluej.editor.font=Courier
# bluej.editor.font=Courier-bold
bluej.editor.font=SansSerif
# bluej.editor.font=SansSerif-bold
# bluej.editor.font=Verdana
# bluej.editor.font=Verdana-bold
# bluej.editor.font=Arial-bold

# fontsize for the editor - this is just the initial default. It can be
# changed in the individual user preferences from within BlueJ.
bluej.editor.fontsize=12

# fonts for printing
bluej.fontsize.printText=10
bluej.fontsize.printTitle=14
bluej.fontsize.printInfo=10

# page format for printing


# should we syntax hilight
bluej.syntaxHilighting=true

# class notation style eg. uml or blue
bluej.notation.style=uml
#bluej.notation.style=blue

## debugging: when on, debug output goes to console; when off, to log file
debug=on

##Log Files
bluej.debugLog=debug.log

#######################################################################
## Applets -  preferences for how applets are generated and executed
##
#######################################################################

#appletviewer command
#appletViewer.command=c:\\jdk1.3\\bin\\appletviewer
appletViewer.command=appletviewer

# which jvm browser internal or JDK plugin
bluej.applet.jvm=html

# applet type standard AWT Applet or Swing based JApplet
bluej.applet.type=applet
#bluej.applet.type=japplet

