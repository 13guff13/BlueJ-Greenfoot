$COMMENT
    <head>
        <title>$TITLE Applet</title>
    </head>
    <body>
        <h1>$TITLE Applet</h1>
        <hr>
        <applet code="$CLASSFILE" 
            width=$APPLETWIDTH 
            height=$APPLETHEIGHT
